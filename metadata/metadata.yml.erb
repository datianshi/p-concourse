---
name: <%= product_name %>
product_version: <%= product_version %>
metadata_version: "1.5"
target_installer_version: 1.4.0.0
stemcell_criteria:
  os: ubuntu-trusty
  requires_cpi: false
  version: '3202'
releases:
- name: <%= releases["concourse"].name %>
  file: <%= releases["concourse"].file %>
  version: <%= releases["concourse"].version %>
- name: <%= releases["garden-linux"].name %>
  file: <%= releases["garden-linux"].file %>
  version: <%= releases["garden-linux"].version %>

provides_product_versions:
  - name:    <%= product_name %>
    version: <%= product_version %>

label:       Concourse
description: CI that scales with your project
icon_image: 
rank: 200

form_types:
- name: externel_url
  label: Externel Url
  description: Externel Url
  property_inputs:
    - reference: .properties.externel_url
      label: Externel URL
      description: Externel URL

property_blueprints:
- name: externel_url
  type: string
  label: Externel Url
  configurable: true

job_types:
- name: discovery
  templates:
  - name: consul-agent
    release: <%= releases["concourse"].name %>
  resource_label: Discovery
  description: Discovery enables discovery of components.
  resource_definitions:
  - name: ram
    type: integer
    label: RAM
    configurable: true
    default: 2048
  - name: ephemeral_disk
    type: integer
    label: Ephemeral Disk
    configurable: true
    default: 2048
  - name: persistent_disk
    type: integer
    label: Persistent Disk
    configurable: true
    default: 1024
  - name: cpu
    type: integer
    label: CPU
    configurable: true
    default: 1
  static_ip: 1
  dynamic_ip: 0
  max_in_flight: 1
  instance_definitions:
  - name: instances
    type: integer
    label: Instances
    default: 1
    configurable: false
  property_blueprints:
  - name: vm_credentials
    type: salted_credentials
    label: VM credentials
    default:
      identity: vcap
  manifest: |
    consul:
      agent:
        mode: server

- name: web
  templates:
  - name: consul-agent
    release: <%= releases["concourse"].name %>
  - name: atc
    release: <%= releases["concourse"].name %>
  - name: tsa
    release: <%= releases["concourse"].name %>
  resource_label: Web
  description: Web contains ATC (Air Traffic Controller) which provides UI and API
    access. It is responsible for scheduling builds.
  resource_definitions:
  - name: ram
    type: integer
    label: RAM
    configurable: true
    default: 4096
  - name: ephemeral_disk
    type: integer
    label: Ephemeral Disk
    configurable: true
    default: 2048
  - name: persistent_disk
    type: integer
    label: Persistent Disk
    configurable: false
    default: 0
  - name: cpu
    type: integer
    label: CPU
    configurable: true
    default: 2
  static_ip: 1
  dynamic_ip: 0
  max_in_flight: 1
  instance_definitions:
  - name: instances
    type: integer
    label: Instances
    default: 1
    configurable: true
  property_blueprints:
  - name: vm_credentials
    type: salted_credentials
    label: VM credentials
    default:
      identity: vcap
  - name: web_credentials
    type: simple_credentials
    label: Web credentials
    default:
      identity: admin
  manifest: |
    atc:
      basic_auth_username: (( web_credentials.identity ))
      basic_auth_password: (( web_credentials.password ))

      postgresql:
        database: atc
        role:
          name: (( .db.database_credentials.identity ))
          password: (( .db.database_credentials.password ))

      publicly_viewable: false
      external_url: (( .properties.externel_url.value ))

    tsa:
      atc:
        username: (( web_credentials.identity ))
        password: (( web_credentials.password ))

    consul:
      agent:
        servers:
          lan: (( .discovery.ips ))

- name: db
  templates:
  - name: consul-agent
    release: <%= releases["concourse"].name %>
  - name: postgresql
    release: <%= releases["concourse"].name %>
  resource_label: PostgreSQL
  description: The world's most advanced open source database.
  resource_definitions:
  - name: ram
    type: integer
    label: RAM
    configurable: true
    default: 4096
  - name: ephemeral_disk
    type: integer
    label: Ephemeral Disk
    configurable: true
    default: 2048
  - name: persistent_disk
    type: integer
    label: Persistent Disk
    configurable: true
    default: 10240
  - name: cpu
    type: integer
    label: CPU
    configurable: true
    default: 2
  static_ip: 1
  dynamic_ip: 0
  max_in_flight: 1
  instance_definitions:
  - name: instances
    type: integer
    label: Instances
    default: 1
    configurable: false
  property_blueprints:
  - name: vm_credentials
    type: salted_credentials
    label: VM credentials
    configurable: false
    default:
      identity: vcap
  - name: database_credentials
    type: simple_credentials
    label: Database credentials
    default:
      identity: atc
  manifest: |
    postgresql:
      databases:
      - name: atc
      roles:
      - name: (( database_credentials.identity ))
        password: (( database_credentials.password ))

    consul:
      agent:
        servers:
          lan: (( .discovery.ips ))

- name: worker
  templates:
  - name: consul-agent
    release: <%= releases["concourse"].name %>
  - name: groundcrew
    release: <%= releases["concourse"].name %>
  - name: garden
    release: <%= releases["garden-linux"].name %>
  resource_label: Worker
  description: Worker manages containers within which builds are run
  resource_definitions:
  - name: ram
    type: integer
    label: RAM
    configurable: true
    default: 4096
  - name: ephemeral_disk
    type: integer
    label: Ephemeral Disk
    configurable: true
    default: 32768
  - name: persistent_disk
    type: integer
    label: Persistent Disk
    configurable: false
    default: 0
  - name: cpu
    type: integer
    label: CPU
    configurable: true
    default: 2
  static_ip: 1
  dynamic_ip: 0
  max_in_flight: 1
  instance_definitions:
  - name: instances
    type: integer
    label: Instances
    configurable: true
    default: 1
  property_blueprints:
  - name: vm_credentials
    type: salted_credentials
    label: VM credentials
    configurable: false
    default:
      identity: vcap
  manifest: |
    garden:
      listen_address: 0.0.0.0:7777
      listen_network: tcp

    consul:
      agent:
        servers:
          lan: (( .discovery.ips ))

- name: compilation
  resource_label: Compilation
  description: Compilation
  resource_definitions:
  - name: ram
    type: integer
    label: RAM
    configurable: true
    default: 4096
  - name: ephemeral_disk
    type: integer
    label: Ephemeral Disk
    configurable: true
    default: 8192
  - name: persistent_disk
    type: integer
    label: Persistent Disk
    configurable: false
    default: 0
  - name: cpu
    type: integer
    label: CPU
    configurable: true
    default: 4
  static_ip: 0
  dynamic_ip: 1
  instance_definitions:
  - name: instances
    type: integer
    label: Instances
    default: 3
    configurable: true
